{% extends 'auth.base' %}

{% block title %}{{ trans('auth.register.title') }}{% endblock %}

{% block content %}
  <p class="login-box-msg">
    {{ trans('auth.register.message', {sitename: site_name}) }}
  </p>
  <main></main>
  {{ include('auth.oauth') }}
{% endblock %}

{% block before_foot %}
  <script>
    Object.defineProperty(blessing, 'extra', {
      configurable: false,
      get: () => Object.freeze({{ extra|json_encode|raw }})
    })
  </script>
{% endblock %}
